{
  "name": "etabli",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:client": "node client.js",
    "build": "tsc",
    "build:client": "tsc ./client.ts",
    "watch": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "watch:client": "tsc-watch ./client.ts --esModuleInterop --onSuccess \"node ./client.js\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Florian LEROUX",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floriaaan/etabli.git"
  },
  "license": "GNU-GPL-3.0",
  "homepage": "https://github.com/floriaaan/etabli#readme",
  "bugs": {
    "url": "https://github.com/floriaaan/etabli/issues"
  },
  "_moduleAliases": {
    "@etabli/core": "dist/core",
    "@etabli/classes": "dist/core/classes",
    "@etabli/utils": "dist/utils",
    "@etabli/config": "dist/config",
    "mods": "mods"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.6",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.3",
    "module-alias": "^2.2.2",
    "ts-jest": "^28.0.7",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.4",
    "parse-duration": "^1.0.2",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "uuid": "^8.3.2"
  }
}
